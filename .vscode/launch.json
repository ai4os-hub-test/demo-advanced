{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        { // Deep as a Service debug
            "name": "Python: DEEPaaS",
            "type": "python",
            "request": "launch",
            "module": "deepaas.cmd.run",
            "args": [ "--config-file=deepaas.conf" ],
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { // Data pre processing debug
            "name": "Python: data.make_convolution",
            "type": "python",
            "request": "launch",
            "module": "deepaas_full.data.make_convolution",
            "args": [
                "--output=debugging_data.npz",
                "--verbosity=DEBUG",
                "data/raw/t10k-images-idx3-ubyte.gz",
                "data/raw/t10k-labels-idx1-ubyte.gz",
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { // Model creation autoencoder debug
            "name": "Python: models.make_autoencoder",
            "type": "python",
            "request": "launch",
            "module": "deepaas_full.models.make_autoencoder",
            "args": [
                "--name=debug_autoencoder",
                "--verbosity=DEBUG",
                "--latent_dim=64",
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { // Model creation convolution debug
            "name": "Python: models.make_convolution",
            "type": "python",
            "request": "launch",
            "module": "deepaas_full.models.make_convolution",
            "args": [
                "--name=debug_model",
                "--verbosity=DEBUG",
                "--dropout_factor=0.4",
                "--learning_rate=1e-2",
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { //  Model training debug
            "name": "Python: models.train_model",
            "type": "python",
            "request": "launch",
            "module": "deepaas_full.models.train_model",
            "args": [
                "--verbosity=DEBUG",
                "debug_model",
                "--version=Staging",
                "data/processed/t10k-convolution.npz",
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { // Visualisation of MNIST encoded data debug
            "name": "Python: visualization.plot_encoding",
            "type": "python",
            "request": "launch",
            "module": "deepaas_full.visualization.plot_encoding",
            "args": [
                "--images=10",
                "--verbosity=DEBUG",
                "debug_autoencoder",
                "--version=Staging",
                "data/raw/t10k-images-idx3-ubyte.gz",
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { // Visualisation of MNIST raw data debug
            "name": "Python: visualization.plot_samples",
            "type": "python",
            "request": "launch",
            "module": "deepaas_full.visualization.plot_samples",
            "args": [
                "--images=10",
                "--verbosity=DEBUG",
                "data/raw/t10k-images-idx3-ubyte.gz",
                "data/raw/t10k-labels-idx1-ubyte.gz",
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        { // Training workflow debug
            "name": "Python: Training workflow",
            "type": "python",
            "request": "launch",
            "program": "flows/training.py",
            "args": [
                "models:/borja-MNIST/1",
                "data/t10k-dataset.npz",
            ],
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        { // Current file debug
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
        }
    ]
}